        "script": "dummy10.sh",
        "run_next":  "step11"
    }, 

    "step11": {  
        "step_num": 11,
        "job_name":  "mark-gen3-release-as-released",  
        "job_params": {
            "RELEASE_NAME":"{{release_name}}",
            "JIRA_PROJECT":"QAT" 
        },  
        "schedule":  null,
        "script": "dummy11.sh",
        "run_next": null
    }
