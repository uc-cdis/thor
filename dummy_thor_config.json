{
    "step1": {
        "step_num": 1,
        "job_name":  "create-gen3-release-in-jira",  
        "job_params": {
            "RELEASE_VERSION":"{{release_name}}", 
            "JIRA_PROJECT": "QAT"
        },  
        "schedule":  "0 21 8-15 * 5",  
        "script": "dummy1.sh",
        "run_next":  "step2"
    },   

    "step2": {  
        "step_num": 2,
        "job_name":  "create-gen3-release-candidate-branch",  
        "job_params": {
            "RELEASE_VERSION":"{{release_name}}",
            "FORK_FROM":"main"
        },  
        "schedule": null,  
        "script": "dummy2.sh",
        "run_next":  "step3"
    },   

    "step3": {  
        "step_num": 3,
        "job_name":  "check-quay-image",  
